{
  "name": "@orgsoft/concurrently",
  "description": "Run concurrent shell commands",
  "exports": "./mod.ts",
  "version": "0.1.1",
  "tasks": {
    "install": "deno install -frAg -n concurrently concurrently.ts",
    "install:dev": "deno install -frAg -n concurrently concurrently.ts",
    "install:safer": "deno install -frg --allow-env=SHELL --allow-run concurrently.ts",
    "install:safer:dev": "deno install -frg --allow-env=SHELL --allow-run concurrently.ts"
  }
}
